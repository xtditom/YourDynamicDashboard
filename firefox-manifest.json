{
  "manifest_version": 3,
  "name": "YourDynamicDashboard",
  "version": "2.1.0",
  "description": "A dynamic and personalized new tab dashboard.",
  "author": "Ditom Baroi Antu",
  "homepage_url": "https://github.com/xtditom/YourDynamicDashboard",
  "short_name": "YDD",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "your-dynamic-dashboard@ditom.com",
      "strict_min_version": "140.0",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "assets/icons/*.png",
        "assets/search-tools/*.png",
        "assets/ai-tools/*.png",
        "assets/google-apps/*.png",
        "assets/socials/*.png"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
