{
  "manifest_version": 3,
  "name": "YourDynamicDashboard",
  "version": "1.0.0",
  "description": "A dynamic and personalized new tab dashboard.",
  "author": "Ditom Baroi Antu",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "geolocation"
  ],
  "host_permissions": [
    "https://*.google.com/"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icons/*.png",
        "Search Tools/*.png",
        "ai-tools/*.png",
        "G Apps/*.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}